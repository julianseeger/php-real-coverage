{
  "name": "julianseeger/php-real-coverage",
  "description": "Calculate the real coverage based on an existing php-code-coverage report",
  "minimum-stability": "stable",
  "license": "GPL v2",
  "authors": [
    {
      "name": "Julian Seeger",
      "email": "seeger.julian@gmail.com"
    }
  ],
  "bin": [
    "composer/bin/php-real-coverage"
  ],
  "require": {
    "phpunit/phpunit": "<4"
  },
  "require-dev": {
    "phing/phing": "*",
    "squizlabs/php_codesniffer": "*"
  },
  "suggest": {
    "phpunit/php-code-coverage": "*",
    "ext-xdebug": ">=2.1"
  },
  "autoload": {
    "psr-4": {"PHPRealCoverage\\": "src/PHPRealCoverage/"}
  }
}